{
  "rules": {
    ".read": false,
    ".write": false,
    
    "teacherDashboard": {
      "teacherDashboards": {
        "$teacherId": {
          ".read": "auth != null && auth.uid == $teacherId",
          ".write": "auth != null && auth.uid == $teacherId"
        }
      }
    },
    
    "leaderboards": {
      ".read": "auth != null",
      ".write": false
    },
    
    "users": {
      "$uid": {
        ".read": "auth != null && auth.uid == $uid",
        ".write": "auth != null && auth.uid == $uid"
      }
    },
    
    "courses": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    
    "tasks": {
      ".read": "auth != null",
      ".write": "auth != null"
    }
  }
}

